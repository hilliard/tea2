<h1>List Users</h1>

  <table class="table3">
    <thead>
    <tr>
      <th>Username</th>
      <!-- <th>Role</th> -->
      <th>Signed Up On</th>
      <th>Last Signed In On</th>
    </tr>
    </thead>
    <% for user in @users %>
        <tr>
          <td><%= user.username %></td>
          <td><%#= user.roles_mask %></td>
          <td><%= user.signed_up_on %></td>
          <td><%= user.last_signed_in_on %></td>
          <td><%= link_to 'Show', account_settings_path %></td>
          <td><%= link_to 'Edit',  account_settings_path %></td>
          <td><%= link_to 'Delete', user, :data => { :confirm => 'Are You Sure?' }, :method => :delete %></td>
        </tr>
    <% end %>
  </table>

