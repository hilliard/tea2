<!DOCTYPE html>
<html>
    <head>
      <title>Test Execution Application</title>
     <%= stylesheet_link_tag "application", media: "all", "data-turbolinks-track" => true %>
      <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
      <meta name="viewport" content="width=device-width, initial-scale=1.0">
      <%= csrf_meta_tags %>
      <%= render 'layouts/shim' %>
      <%= render 'layouts/header' %>
    </head>

    <body>

    <div class="container-fluid">
      <p>
      <% if current_user %>
          <!-- <h2>Logged in as <%= current_user.username %>.</h2> -->
          <%#= link_to "Sign Out", signed_out_path %>
          <%#= link_to "Account Settings", account_settings_path %>
          <%#= link_to "Admin", admin_users_path %>
      <% else %>
          <!-- <%= link_to "Sign up", new_user_path %> or -->
          <!-- <%= link_to "Sign In", sign_in_path %>  -->
      <% end %>
      </p>
    </div>


    <% flash.each do |name, msg| %>
        <%= content_tag :div, msg, :id => "flash_#{name}" %>
    <% end %>

    <%= yield %>



    <%= debug(params) if Rails.env.development? %>


    </body>
</html>

<%= render 'layouts/footer' %>